{
	"name" : "hanoivip/admin",
	"description" : "Administrative Component",
	"type" : "library",
	"repositories" : [
		{
			"type" : "path",
			"url" : "../payment-client",
			"options" : {
				"symlink" : true
			}
		}, {
                        "type" : "path",
                        "url" : "../game2",
                        "options" : {
                                "symlink" : true
                        }
                },
	],
	"require" : {
		"php" : ">=7.0.0",
		"fideloper/proxy" : "~4.0",
		"guzzlehttp/guzzle" : "^6.3",
		"laravel/framework" : "5.5.*",
		"laravel/tinker" : "~1.0",
		"mervick/curl-helper" : "^1.1",
		"hanoivip/payment-client" : "dev-master",
		"hanoivip/game" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\Admin\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Admin\\LibServiceProvider"
			]
		}
	}
}
