{
	"name" : "hanoivip/admin",
	"description" : "Administrative Component",
	"type" : "library",
    "repositories" : [{
			"type" : "path",
			"url" : "../payment-contract",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../game",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../events",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../game-contracts",
			"options" : {
				"symlink" : true
			}
		}
	],
	"require" : {
		"php" : ">=7.0.0",
		"fideloper/proxy" : "~4.0",
		"guzzlehttp/guzzle" : "^6.3",
		"laravel/framework" : ">=5.5",
		"laravel/tinker" : "~1.0",
		"mervick/curl-helper" : "^2.2",
		"hanoivip/payment-contract" : "dev-main",
		"hanoivip/game" : "dev-master",
		"hanoivip/game-contracts" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\Admin\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Admin\\LibServiceProvider"
			]
		}
	}
}